    "description": "Just Another Router Library for React",
    "main": "index.cjs.js",
    "module": "index.mjs",
    "jsnext:main": "index.mjs",
    "browser": "index.umd.js",
    "scripts": {
        "test": "jest",
        "scrub": "babel-node ./build/scrub.js",
