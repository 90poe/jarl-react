{
    "name": "jarl-react-native",
    "version": "1.0.0-alpha.15",
    "description": "React Native support for JARL",
    "main": "main.js",
    "sideEffects": false,
    "scripts": {
        "test": "jest --watch",
        "ci-test": "jest --colors",
        "clean": "git clean -fdX .",
        "lint": "eslint . --max-warnings 0 --color",
        "coverage": "jest --coverage"
    },
    "files": [
        "index.js",
        "Link.js",
        "NativeProvider.js"
    ],
    "jest": {
        "setupFiles": [
            "raf/polyfill",
            "./tools/configureJest.js"
        ],
        "testRegex": "__tests__/.+\\.test\\.js$",
        "preset": "react-native",
        "collectCoverageFrom": [
            "*.js"
        ]
    },
    "keywords": [
        "react",
        "reactjs",
        "native",
        "routing",
        "router",
        "components",
        "tools",
        "history"
    ],
    "author": "Peter Hurst",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/downplay/jarl-react.git"
    },
    "peerDependencies": {
        "react": ">=15.0.0",
        "react-native": ">=0.44.0"
    },
    "dependencies": {
        "history": "^4.7.2",
        "invariant": "^2.2.4",
        "jarl-react": "^1.0.0-alpha.15",
        "prop-types": "^15.6.0",
        "warning": "^3.0.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.1.0",
        "babel-jest": "^22.4.3",
        "babel-plugin-add-react-displayname": "^0.0.4",
        "babel-plugin-dev-expression": "^0.2.1",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-plugin-react-native-web": "^0.5.4",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-es2015-rollup": "^3.0.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-react-native": "^4.0.0",
        "babel-runtime": "^6.26.0",
        "enzyme": "^3.2.0",
        "enzyme-adapter-react-16": "^1.1.0",
        "jest": "^22.4.3",
        "react": "^16.3.1",
        "react-dom": "^16.3.1",
        "react-native": "^0.55.1",
        "react-native-web": "^0.5.4"
    }
}
