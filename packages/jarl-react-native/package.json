{
    "name": "jarl-react-native",
    "version": "0.8.0",
    "description": "React Native support for JARL",
    "main": "index.js",
    "module": "es/index.js",
    "sideEffects": false,
    "scripts": {
        "test": "jest --watch",
        "clean": "git clean -fdX .",
        "lint": "eslint source",
        "coverage": "jest --coverage"
    },
    "files": ["index.js", "Link.js", "NativeProvider.js"],
    "jest": {
        "setupFiles": ["raf/polyfill"],
        "testRegex": "__tests__/.+\\.test\\.js$",
        "preset": "react-native",
