    "keywords": [
        "react",
        "reactjs",
        "native",
        "routing",
        "router",
        "components",
        "tools",
        "history"
    ],
    "author": "Peter Hurst",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/downplay/jarl-react.git"
    },
    "peerDependencies": {
        "react": ">=15",
        "react-native": ">=0.44"
    },
    "dependencies": {
        "prop-types": "^15.6.0",
        "invariant": "^2.2.4",
        "jarl-react": "^0.8.0",
        "warning": "^3.0.0"
    },
    "devDependencies": {
        "babel-jest": "^22.1.0",
        "babel-preset-react-native": "^4.0.0",
        "enzyme": "^3.2.0",
        "enzyme-adapter-react-16": "^1.1.0",
        "jest": "^22.1.4",
        "react": "^16.2.0",
        "react-native": "^0.49.5"
    }
